<?import javafx.stage.Stage?>

<Stage title="Eggventory" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <scene>
        <Scene>
            <AnchorPane prefHeight="800.0" prefWidth="720.0">
            <children>
               <VBox prefHeight="200.0" prefWidth="100.0" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                  <children>
                     <ScrollPane fx:id="outputTableScroll" fitToHeight="true" fitToWidth="true" focusTraversable="false" maxHeight="1.7976931348623157E308" prefHeight="400.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                        <content>
                           <TableView fx:id="outputTable" editable="true" focusTraversable="false" maxHeight="1.7976931348623157E308" minHeight="-Infinity" prefHeight="648.0" prefWidth="685.0" />
                        </content>
                     </ScrollPane>
                     <Separator prefWidth="200.0" />
                     <TextArea fx:id="outputField" editable="false" focusTraversable="false" maxHeight="1.7976931348623157E308" prefHeight="250.0" prefWidth="710.0" wrapText="true" VBox.vgrow="NEVER">
                        <font>
                           <Font name="Monospaced Bold" size="10.0" />
                        </font>
                     </TextArea>
                     <Group focusTraversable="true">
                        <children>
                           <AnchorPane fx:id="rectPane" prefHeight="20.0" prefWidth="200.0">
                              <children>
                                 <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="20.0" stroke="BLACK" strokeType="INSIDE" width="720.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                              </children>
                           </AnchorPane>
                           <TextFlow fx:id="textFlow" maxWidth="1.7976931348623157E308" minHeight="-Infinity" prefHeight="20.0" prefWidth="720.0">
                              <padding>
                                 <Insets left="5.0" top="1.0" />
                              </padding>
                           </TextFlow>
                        </children>
                     </Group>
                  </children>
               </VBox>
            </children>
            <padding>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding></AnchorPane>
        </Scene>
    </scene>
</Stage>
